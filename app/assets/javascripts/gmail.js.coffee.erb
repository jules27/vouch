jQuery ->
  $("#google_log_in").on "click", (e) ->
    e.preventDefault()
    $(".friend-list-error").hide()
    $(".friend-list-notice").hide()
    $(".friend-list-success").hide()

    auth_url = 'https://accounts.google.com/o/oauth2/auth'
    scope = '?scope=https%3A%2F%2Fwww.google.com%2Fm8%2Ffeeds'
    state = '&state=something'
    response_type = '&response_type=token'
    client_id = '&client_id=1058703828317.apps.googleusercontent.com'
    redirect_uri = '&redirect_uri=http%3A%2F%2Flocalhost%3A3000%2Foauth2callback'

    oauth_window = 
              window.open(auth_url + scope + response_type + client_id + redirect_uri,
                '_gmail',
                'height=400,width=600,status=no,menubar=no,location=no,toolbar=no')
